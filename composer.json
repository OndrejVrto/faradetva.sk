{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "diglactic/laravel-breadcrumbs": "^v7.1.1",
        "enlightn/enlightn": "^1.25",
        "guzzlehttp/guzzle": "^7.2",
        "haruncpi/laravel-user-activity": "^1.0",
        "intervention/image": "^2.7",
        "jeroennoten/laravel-adminlte": "^v3.8.0",
        "lab404/laravel-impersonate": "^1.7.3",
        "laravel/framework": "^9",
        "laravel/sanctum": "^2.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^3.4",
        "livewire/livewire": "^2.10",
        "meilisearch/meilisearch-php": "^0.23.1",
        "nanigans/single-table-inheritance": "~1.0",
        "spatie/crawler": "^7.1",
        "spatie/laravel-collection-macros": "^7.10",
        "spatie/laravel-csp": "^2.7",
        "spatie/laravel-medialibrary": "^10.0",
        "spatie/laravel-permission": "^5.5",
        "spatie/laravel-site-search": "^1.2.0",
        "tinymce/tinymce": "^5.10",
        "unisharp/laravel-filemanager": "^2.4",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "beyondcode/laravel-query-detector": "^1.6.0",
        "fakerphp/faker": "^1.9.1",
        "glhd/laravel-dumper": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "exclude-from-classmap": [
            "Yoeunes\\Toastr"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
		"files": [
            "app/Overrides/toastr/ToastrServiceProvider.php",
            "app/Overrides/toastr/Toastr.php",

			"app/Http/Helpers/BeautifyHtml.php",
			"app/Http/Helpers/DataFormater.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
			"Illuminate\\Foundation\\ComposerScripts::postUpdate",
			"@php artisan ide-helper:generate",
			"@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
